{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ahdf01"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1'"
		},
		"NginxAccessLog_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'NginxAccessLog'"
		},
		"ahazuresa01_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ahazuresa01'"
		},
		"ahsa01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ahsa01'"
		},
		"ahsqldb01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ahsqldb01'"
		},
		"NginxAccessLog_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\nginx-1.24.0\\logs"
		},
		"NginxAccessLog_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "ahuser"
		},
		"ahazuresa01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ahazuresa01.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ahdf01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"ahazuresa01_accountKey": {
						"value": "[parameters('ahazuresa01_accountKey')]"
					},
					"ahsa01_connectionString": {
						"value": "[parameters('ahsa01_connectionString')]"
					},
					"ahsqldb01_connectionString": {
						"value": "[parameters('ahsqldb01_connectionString')]"
					},
					"ahazuresa01_properties_typeProperties_url": {
						"value": "[parameters('ahazuresa01_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ahdf01_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"NginxAccessLog_password": {
						"value": "[parameters('NginxAccessLog_password')]"
					},
					"NginxAccessLog_properties_typeProperties_host": {
						"value": "[parameters('NginxAccessLog_properties_typeProperties_host')]"
					},
					"NginxAccessLog_properties_typeProperties_userId": {
						"value": "[parameters('NginxAccessLog_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ahdf01_ArmTemplate_0')]"
			]
		},
		{
			"name": "ahdf01_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ahdf01_ArmTemplate_1')]"
			]
		}
	]
}